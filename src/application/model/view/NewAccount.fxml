<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="250.0" prefWidth="450.0" stylesheets="@Styles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.application.model.Controller.NewAccountController">
   <children>
      <GridPane layoutX="109.0" layoutY="60.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="45.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="213.5" minWidth="10.0" prefWidth="180.5" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="254.5" minWidth="10.0" prefWidth="254.5" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="User Name" />
            <Label text="Password" GridPane.rowIndex="2" />
            <Label text="Repeat Password" GridPane.rowIndex="3" />
            <TextField fx:id="userNameField" promptText=" User Name" GridPane.columnIndex="1">
               <padding>
                  <Insets bottom="3.0" top="3.0" />
               </padding>
               <opaqueInsets>
                  <Insets left="5.0" />
               </opaqueInsets>
            </TextField>
            <TextField fx:id="passwordField" promptText=" Password" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <padding>
                  <Insets bottom="3.0" top="3.0" />
               </padding>
               <opaqueInsets>
                  <Insets left="5.0" />
               </opaqueInsets>
            </TextField>
            <TextField fx:id="repeatPasswordField" promptText=" Repeat Password" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <padding>
                  <Insets bottom="3.0" top="3.0" />
               </padding>
               <opaqueInsets>
                  <Insets left="5.0" />
               </opaqueInsets>
            </TextField>
         </children>
      </GridPane>
      <ButtonBar layoutX="186.0" layoutY="246.0" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="10.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#handleOk" stylesheets="@Styles.css" text="OK" />
            <Button cancelButton="true" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
        </buttons>
      </ButtonBar>
      <Label layoutX="14.0" layoutY="14.0" styleClass="column-header" text="Create new User-Account" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="5.0" />
   </children>
</AnchorPane>
